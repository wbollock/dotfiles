{
  "search": {
    "include_ignored": true
  },
  "buffer_line_height": "standard",
  "ui_font_family": "DroidSansMono Nerd Font",
  "buffer_font_family": "DroidSansMono Nerd Font",
  "buffer_font_features": {
    "calt": 0,
    "liga": 0,
  },
  "agent_servers": {
    "github-copilot": {
      "type": "registry",
    },
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "session": {
    "trust_all_worktrees": true,
  },
  "base_keymap": "VSCode",
  "autosave": {
    "after_delay": {
      "milliseconds": 1000,
    },
  },
  "soft_wrap": "editor_width",
  "ui_font_size": 16,
  "buffer_font_size": 14.0,
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Zedokai Darker (Filter Spectrum)",
  },
}
