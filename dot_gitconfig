[include]
	path = ~/.gitconfig.work
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[core]
	editor = vim
[push]
	default = current
[diff]
	algorithm = patience
	context = 5
[includeIf "gitdir:~/repos/**"]
	path = ~/.gitconfig.work
[includeIf "gitdir:~/repos/github/**"]
	path = ~/.gitconfig.personal
